{
    "name": "sigmie/app",
    "type": "project",
    "description": "Most of search application",
    "keywords": [
        "framework",
        "laravel",
        "sigmie"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://packages.sigmie.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "configcat/configcat-client": "^2.0",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "google/cloud-error-reporting": "^0.16.2",
        "google/cloud-logging": "^1.20",
        "guzzlehttp/guzzle": "^6.4",
        "inertiajs/inertia-laravel": "^0.2.5",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/nova": "^3.0",
        "laravel/scout": "^8.0",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "nunomaduro/collision": "^4.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~4.0",
        "ramsey/uuid": "^3.9",
        "sentry/sentry-laravel": "^1.5",
        "sigmie/app-core": "dev-master",
        "tightenco/ziggy": "^0.9.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^0.5.2",
        "phpstan/phpstan": "^0.12.6",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "chmod -R 755 bootstrap\/cache",
            "php artisan cache:clear"
        ]
    }
}
