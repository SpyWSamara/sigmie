{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://packages.mos-sigma.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "beyondcode/laravel-dump-server": "^1",
        "configcat/configcat-client": "^2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.4",
        "laravel/cashier": "^10.3",
        "laravel/framework": "^6.17",
        "laravel/horizon": "^3.4",
        "laravel/nova": "~2.0",
        "laravel/scout": "^7.2",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^2",
        "laravel/tinker": "^1.0",
        "mos-sigma/mos": "dev-master",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.9",
        "sentry/sentry-laravel": "1.5.0",
        "spatie/laravel-cookie-consent": "^2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.5.2",
        "phpstan/phpstan": "^0.12.6",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": ["https"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
