{
    "name": "sigmie/app",
    "type": "project",
    "description": "Sigmie application",
    "keywords": [
        "framework",
        "sigmie"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://packages.sigmie.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sigmie/sigmie.git"
        },
        {
            "type": "path",
            "url": "./packages/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.0",
        "composer-runtime-api": "^2.0",
        "calebporzio/sushi": "^2.1",
        "fakerphp/faker": "^1.13",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.3.6",
        "laravel/cashier-paddle": "^1.0@beta",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/nova": "^3.0",
        "laravel/sanctum": "^2.4",
        "laravel/scout": "^8.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "nunomaduro/collision": "^5.0",
        "pusher/pusher-php-server": "~4.0",
        "sigmie/app-core": "^0.10.0",
        "sigmie/nova-feature-flags": "@dev",
        "sigmie/sigmie-laravel-testing": "@dev",
        "sigmie/sigmie": "dev-develop",
        "tightenco/ziggy": "^0.9.3"
    },
    "require-dev": {
        "claudiodekker/inertia-laravel-testing": "^1.2",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/larastan": "^0.6.1",
        "phpstan/phpstan": "^0.12.6",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ray": "^1.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [],
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "chmod -R 755 bootstrap/cache",
            "php artisan cache:clear"
        ]
    }
}
