includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app
        - tests

    ignoreErrors:
    - '/Call to an undefined method.*/'
    - '/Access to an undefined property.*/'

    # The level 8 is the highest level
    level: 5

    excludes_analyse:
        - ./*/*/FileToBeExcluded.php

    checkMissingIterableValueType: false
